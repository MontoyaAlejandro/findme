<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Teal_50"
    tools:context=".Lugares">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="FindMe"
        android:textColor="#152A9F"
        android:textSize="38dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="243dp"
        android:layout_height="158dp"
        android:layout_marginTop="88dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/logofinal" />

    <EditText
        android:id="@+id/lugar"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_gravity="fill_vertical"
        android:layout_margin="20dp"
        android:width="250dp"
        android:ems="10"
        android:gravity="clip_vertical|fill_vertical"
        android:hint="Lugar"
        android:inputType="textPersonName"
        android:padding="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        tools:visibility="visible" />

    <EditText
        android:id="@+id/descripcion"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_gravity="fill_vertical"
        android:layout_margin="20dp"
        android:width="250dp"
        android:ems="10"
        android:gravity="clip_vertical|fill_vertical"
        android:hint="Descripcion"
        android:inputType="textMultiLine"
        android:padding="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lugar"
        tools:visibility="visible" />

    <EditText
        android:id="@+id/longitud"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_gravity="fill_vertical"
        android:layout_margin="20dp"
        android:width="250dp"
        android:ems="10"
        android:gravity="clip_vertical|fill_vertical"
        android:hint="Longitud"
        android:inputType="numberDecimal"
        android:padding="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/descripcion"
        tools:visibility="visible" />

    <EditText
        android:id="@+id/latitud"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_gravity="fill_vertical"
        android:layout_margin="20dp"
        android:width="250dp"
        android:ems="10"
        android:gravity="clip_vertical|fill_vertical"
        android:hint="Latitud"
        android:inputType="numberDecimal"
        android:padding="8dp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/longitud"
        tools:visibility="visible" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/agregar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:scaleX="2"
        android:scaleY="2"
        app:backgroundTint="@color/Teal_700"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.925"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:layout_editor_absoluteX="177dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:scaleX="1"
        android:scaleY="1"
        app:backgroundTint="@color/cardview_shadow_start_color"
        app:srcCompat="@android:drawable/ic_menu_mylocation"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="562dp" />

</androidx.constraintlayout.widget.ConstraintLayout>